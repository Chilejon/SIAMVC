<!doctype html>
<html lang="en" class="govuk-template flexbox no-flexboxtweener">

<head>
	<meta charset="utf-8" />
	<meta http-equiv="x-ua-compatible" content="ie=edge" />
	<meta name="viewport" content="width=device-width, initial-scale=1" />
	<meta name="theme-color" content="#006666">
	<link rel="shortcut icon" type="image/png" href="~/assets/images/favicon-sg.png" media="print" />
	<title>Stockport image archive - Stockport Council</title>
	<link href="https://design-system.stockport.gov.uk/int/1/smbc-frontend.min.css" rel="stylesheet" />
	<link href="~/assets/css/form-builder.css" rel="stylesheet" />
	<script src="https://www.google.com/recaptcha/api.js" async defer></script>
	<script type="text/javascript">

		//$('img[src="http://interactive.stockport.gov.uk/stockportimagearchive/SIA/thumbnails/33577.jpg"]').parents('div:first').hide();
		function loadImage() {
			var images = document.getElementsByClassName("Thumbnail");
//			console.log(images.length);
			for (i = 0; i < images.length; i++) {
				if (images[i].src == "https://localhost:44368/assets/images/notfound.jpg") {
//					console.log("found one");
					images[i].closest('.govuk-summary-list__row').style.display = "none";
				}
			} 
		}

		function myFunction(item) {
			console.log(item);
		}


	</script>

</head>

<body class="govuk-template__body">
	<script>document.body.className = ((document.body.className) ? document.body.className + ' js-enabled' : 'js-enabled');</script>

	<div role="navigation">
		<a href="#main-content" class="govuk-skip-link">Skip to main content</a>
	</div>

	<partial name="Header" />

	<div class="govuk-width-container">
		<main class="govuk-main-wrapper " id="main-content">
			<div class="govuk-grid-row">
				<div class="govuk-grid-column-two-thirds">
					@RenderBody()
				</div>
			</div>
		</main>
	</div>

	<partial name="Footer" />
	<script type="text/javascript" src="https://design-system.stockport.gov.uk/int/1/smbc-frontend.min.js"></script>
	<script>window.GOVUKFrontend.initAll()</script>
	<script>window.SMBCFrontend.initAll()</script>
</body>
</html>