@model IEnumerable<SIAMVC.Models.Photograph>

@{
    ViewData["Title"] = "SearchClassNo";
}

<h1>SearchClassNo</h1>



<table class="table">
    <thead>
        <tr>

            <th>
                @Html.DisplayNameFor(model => model.Title)
            </th>

            <th>
                @Html.DisplayNameFor(model => model.url)
            </th>

        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>



            <td>
                @Html.DisplayFor(modelItem => item.Title)
            </td>

			<td>
				<img src=@item.url style="width:200px" onError="this.onerror=null;this.src='../assets/images/notfound.jpg';" />
			</td>

        </tr>
}
    </tbody>
</table>
